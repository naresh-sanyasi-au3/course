<div class="container">
<div class="card-deck">
{{#each data}}
<div class="card">
<div class="card-body">
<ul class="list-group list-group-flush">
<li class="list-group-item">{{this.name}}</li>
<li class="list-group-item">{{this.email}}</li>
<li class="list-group-item">{{this.phone}}</li>
<li class="list-group-item">{{this.age}}</li>
</ul>
<button class="deleteStudent" type="button" value="{{this._id}}">Delete</button>
</div>
</div>
{{/each}}
</div>
</div>





